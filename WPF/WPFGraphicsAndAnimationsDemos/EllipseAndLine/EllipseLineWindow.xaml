<Window x:Class="EllipseAndLine.EllipseLineWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Ellipse and Line" Height="300" Width="300">
    <StackPanel Orientation="Vertical" Height="260">
        <TextBlock Background="LightGray" Padding="5" Margin="10">Foo</TextBlock>
        <Line Stroke="Green" X1="20" Y1="10" X2="100" Y2="40"
              StrokeDashArray="0.2, 2" StrokeThickness="3" StrokeDashCap="Round"/>
        <TextBlock Background="LightGray" Padding="5" Margin="10">Bar</TextBlock>
        <Line X1="40" Y1="30" X2="200" Y2="0" Stroke="Red" StrokeThickness="10"
              StrokeStartLineCap="Round" StrokeEndLineCap="Round" DataContext="{Binding}" />
        <TextBlock></TextBlock>
        <Line Stroke="Blue" X1="0" X2="1" Stretch="Fill" />
        <TextBlock></TextBlock>
        <Ellipse Width="100" Height="50" Fill="Yellow" Stroke="Black" />
    </StackPanel>
</Window>
